{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "esModuleInterop": true,
    "isolatedModules": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "strict": true,
    "types": ["node"]
  },
  "include": ["./src"]
}
